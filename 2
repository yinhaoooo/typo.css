select
    distinct t_02.puid
from
    PWORKSPACEOBJECT t_01 ,
    PMFG0MEPLANTBOPREVISION t_02 ,
    PSTRUCTURE_REVISIONS t_03 ,
    PPSBOMVIEWREVISION t_04 ,
    PPSOCCURRENCE t_05 ,
    PPSBOMVIEWREVISION t_06 ,
    PPSOCCURRENCE t_07 ,

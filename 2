    PITEMREVISION t_016 ,
    PMFG0MEPROCLINE t_017 ,
    PPSOCCURRENCE t_018 ,
    PPSBOMVIEWREVISION t_019
where
    ( ( ( ( t_03.pvalu_0 = t_019.puid )
    and ( ( t_019.puid = t_018.rparent_bvru )
    and ( ( t_018.rchild_itemu = t_017.puid )
    and ( ( t_017.puid = t_016.ritems_tagu )
    and ( ( t_015.pvalu_0 = t_04.puid )

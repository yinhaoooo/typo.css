CREATE TABLE pimanrelation (
	puid varchar(15) NOT NULL,
	pfnd0copystableid varchar(128) NULL,
	rsecondary_objectu varchar(15) NOT NULL,
	rsecondary_objectc int8 NOT NULL,
	rrelation_typeu varchar(15) NOT NULL,
	rrelation_typec int8 NOT NULL,
	ruser_datau varchar(15) NULL,
	ruser_datac int8 NULL,
	rprimary_objectu varchar(15) NOT NULL,
	rprimary_objectc int8 NOT NULL,
	pfnd0copystabledate timestamp NULL,
	pfnd0requiresdigitalsign int8 NULL,
	aoid varchar(15) NOT NULL,
	arev_category int8 NOT NULL DEFAULT 48,
	aspace_uid varchar(15) NULL,
	avalid_from timestamp NOT NULL DEFAULT to_date('1900/01/02 00:00:00'::text, 'YYYY/MM/DD HH24:MI:SS'::text),
	avalid_to timestamp NULL
);
CREATE UNIQUE INDEX pipimanrelation ON public.pimanrelation USING btree (puid);
CREATE INDEX pipimanrelation_0 ON public.pimanrelation USING btree (aoid);
CREATE UNIQUE INDEX pipimanrelation_1 ON public.pimanrelation USING btree ((
CASE arev_category
    WHEN 40 THEN rprimary_objectu
    WHEN 48 THEN rprimary_objectu
    WHEN 8 THEN rprimary_objectu
    ELSE NULL::character varying
END), (
CASE arev_category
    WHEN 40 THEN pfnd0copystableid
    WHEN 48 THEN pfnd0copystableid
    WHEN 8 THEN pfnd0copystableid
    ELSE NULL::character varying
END), (
CASE arev_category
    WHEN 40 THEN rrelation_typeu
    WHEN 48 THEN rrelation_typeu
    WHEN 8 THEN rrelation_typeu
    ELSE NULL::character varying
END), (
CASE arev_category
    WHEN 8 THEN aspace_uid
    ELSE NULL::character varying
END));
CREATE UNIQUE INDEX pipimanrelation_10 ON public.pimanrelation USING btree ((
CASE arev_category
    WHEN 48 THEN rprimary_objectu
    WHEN 16 THEN rprimary_objectu
    ELSE NULL::character varying
END), (
CASE arev_category
    WHEN 48 THEN pfnd0copystableid
    WHEN 16 THEN pfnd0copystableid
    ELSE NULL::character varying
END), (
CASE arev_category
    WHEN 48 THEN rrelation_typeu
    WHEN 16 THEN rrelation_typeu
    ELSE NULL::character varying
END));
CREATE INDEX pipimanrelation_11 ON public.pimanrelation USING btree (rprimary_objectu, pfnd0copystableid, rrelation_typeu);
CREATE INDEX pipimanrelation_12 ON public.pimanrelation USING btree (rprimary_objectu, rsecondary_objectu);
CREATE UNIQUE INDEX pipimanrelation_13 ON public.pimanrelation USING btree ((
CASE arev_category
    WHEN 40 THEN rprimary_objectu
    WHEN 48 THEN rprimary_objectu
    WHEN 8 THEN rprimary_objectu
    ELSE NULL::character varying
END), (
CASE arev_category
    WHEN 40 THEN rsecondary_objectu
    WHEN 48 THEN rsecondary_objectu
    WHEN 8 THEN rsecondary_objectu
    ELSE NULL::character varying
END), (
CASE arev_category
    WHEN 40 THEN rrelation_typeu
    WHEN 48 THEN rrelation_typeu
    WHEN 8 THEN rrelation_typeu
    ELSE NULL::character varying
END), (
CASE arev_category
    WHEN 8 THEN aspace_uid
    ELSE NULL::character varying
END));
CREATE INDEX pipimanrelation_2 ON public.pimanrelation USING btree (rsecondary_objectu);
CREATE INDEX pipimanrelation_3 ON public.pimanrelation USING btree (rprimary_objectu, rrelation_typeu, rsecondary_objectu);
CREATE INDEX pipimanrelation_4 ON public.pimanrelation USING btree (rsecondary_objectu);

	pxt_transform_rot00 numeric NULL,
	pxt_transform_rot10 numeric NULL,
	pxt_transform_rot20 numeric NULL,
	pxt_transform_per0 numeric NULL,
	pxt_transform_rot01 numeric NULL,
	pxt_transform_rot11 numeric NULL,
	pxt_transform_rot21 numeric NULL,
	pxt_transform_per1 numeric NULL,
	pxt_transform_rot02 numeric NULL,
	pxt_transform_rot12 numeric NULL,
	pxt_transform_rot22 numeric NULL,
	pxt_transform_per2 numeric NULL,
	pxt_transform_tra0 numeric NULL,
	pxt_transform_tra1 numeric NULL,
	pxt_transform_tra2 numeric NULL,
	pxt_transform_invscale numeric NULL,
	pqtyvalue numeric NULL,
	pnotetext varchar(160) NULL,
	pseqno varchar(15) NULL,
	poccname varchar(32) NULL,
	parchtype varchar(32) NULL,
	parchelemid varchar(128) NULL,
	plogicalpos varchar(32) NULL,
	vla_998_32 int8 NOT NULL DEFAULT 0,
	vla_998_33 int8 NOT NULL DEFAULT 0,
	pref_designator varchar(30) NULL,
	pflagvalue int8 NULL,
	pflagmask int8 NULL,
	poccflags int8 NULL,
	rdata_qualifieru varchar(15) NULL,
	rdata_qualifierc int8 NULL,
	rabs_occu varchar(15) NOT NULL,
	rabs_occc int8 NOT NULL,
	rchilditemu varchar(15) NULL,
	rchilditemc int8 NULL,
	rparent_absoccu varchar(15) NULL,
	rparent_absoccc int8 NULL,
	pdatamask int8 NULL,
	pinstancenumber int8 NULL,
	psource int8 NOT NULL,
	rnotetypeu varchar(15) NULL,
	rnotetypec int8 NULL,
	rchildbvu varchar(15) NULL,
	rchildbvc int8 NULL,
	rocctypeu varchar(15) NULL,
	rocctypec int8 NULL,
	ruom_tagu varchar(15) NULL,
	ruom_tagc int8 NULL,
	rvariantconditionu varchar(15) NULL,
	rvariantconditionc int8 NULL,
	rucc_vcu varchar(15) NULL,
	rucc_vcc int8 NULL,
	rgdeobjectu varchar(15) NULL,
	rgdeobjectc int8 NULL
);
CREATE UNIQUE INDEX pipabsoccdata ON public.pabsoccdata USING btree (puid);
CREATE INDEX pipabsoccdata_0 ON public.pabsoccdata USING btree (rparent_absoccu);
CREATE INDEX pipabsoccdata_1 ON public.pabsoccdata USING btree (rabs_occu);

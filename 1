
-- Drop table

-- DROP TABLE acct_table;

CREATE TABLE acct_table (
	exp_obj_uid varchar(15) NOT NULL,
	led timestamp NOT NULL,
	island_anchor_uid varchar(15) NOT NULL,
	state int8 NOT NULL,
	object_class varchar(32) NULL,
	app_id varchar(15) NULL
);
CREATE INDEX acct_table_app_id_idx ON public.acct_table USING btree (app_id);
CREATE INDEX acct_table_index ON public.acct_table USING btree (exp_obj_uid);
CREATE INDEX acct_table_index2 ON public.acct_table USING btree (island_anchor_uid);
CREATE INDEX acct_table_index3 ON public.acct_table USING btree (state);
CREATE INDEX acct_table_index4 ON public.acct_table USING btree (led);
CREATE INDEX acct_table_index5 ON public.acct_table USING btree (island_anchor_uid, app_id, state);

-- Drop table

-- DROP TABLE acls;

CREATE TABLE acls (
	"object" varchar(27) NOT NULL,
	seq_no int8 NOT NULL,
	puser varchar(27) NULL,
	pgroup varchar(27) NULL,
	access_mask int8 NOT NULL,
	priv_string varchar(64) NULL
);
CREATE UNIQUE INDEX aclsi ON public.acls USING btree (object, seq_no);

-- Drop table

-- DROP TABLE auditlog;

CREATE TABLE auditlog (
	puid varchar(15) NOT NULL,
	objectuid varchar(15) NOT NULL,
	eventtypeuid varchar(15) NOT NULL,
	objectid varchar(128) NULL,
	objectname varchar(128) NOT NULL,
	objectrev varchar(32) NULL,
	auditdefuid varchar(15) NOT NULL,
	eventtypename varchar(32) NOT NULL,
	userid varchar(32) NOT NULL,
	loggeddate timestamp NOT NULL,
	objecttype varchar(32) NOT NULL,
	objectseqno int8 NULL,
	sec_objectuid varchar(15) NULL,
	sec_objecttype varchar(32) NULL,
	sec_objectid varchar(128) NULL,
	sec_objectname varchar(128) NULL,
	sec_objectrev varchar(32) NULL,
	sec_objectseqno int8 NULL,
	errorcode int8 NULL,
	errorstring varchar(256) NULL,
	groupname varchar(256) NOT NULL,
	groupuid varchar(15) NOT NULL,
	CONSTRAINT auditlog_puid_key UNIQUE (puid),
	CONSTRAINT auditlog_puid_key1 UNIQUE (puid),
	CONSTRAINT auditlog_puid_key2 UNIQUE (puid)
);
CREATE UNIQUE INDEX auditlog_puid_pk ON public.auditlog USING btree (puid);
CREATE INDEX auditlogobjectuid ON public.auditlog USING btree (objectuid);
CREATE UNIQUE INDEX pk_log ON public.auditlog USING btree (puid);

